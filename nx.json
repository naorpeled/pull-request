{
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "npmScope": "pull-request",
  "affected": { "defaultBase": "main" },
  "implicitDependencies": {
    "package.json": { "dependencies": "*", "devDependencies": "*" },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/nx-cloud",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"],
        "accessToken": "NzNhNjM1OTQtZmRiYS00NTIxLWIwNWQtYzcwMDM4YmEyNTE4fHJlYWQtd3JpdGU="
      }
    }
  },
  "targetDefaults": { "build": { "dependsOn": ["^build"] } },
  "generators": {
    "@nrwl/react": { "application": { "babel": true } },
    "@nrwl/next": { "application": { "style": "css", "linter": "eslint" } }
  },
  "defaultProject": "pull-request-web"
}
